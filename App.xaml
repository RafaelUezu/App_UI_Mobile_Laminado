<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:App_UI_Mobile_Laminado"
             xmlns:controls="clr-namespace:App_UI_Mobile_Laminado.Commands.Controls"
             x:Class="App_UI_Mobile_Laminado.App">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>

            
            <controls:CurrentShellItemConverter x:Key="CurrentShellItemConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>